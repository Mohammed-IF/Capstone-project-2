<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->


    <!-- Main Site -->
    <main id="site-main">
        <div class="container">
            <div class="box-nav d-flex justify-between">
                <a href="/add-postedServices" class="border-shadow">
                    <span class="text-gradient">New Freelancer<i class="fas fa-user"></i></span>
                </a>
            </div>

            <!-- form handling -->
            <form action="/api/postedServices" method="POST">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>name</th>
                            <th>category</th>
                            <th>description</th>
                        </tr>
                    </thead>
                    <tbody>
                        <section data-position="postedservices">
                            <h2>postedservices</h2>
                      
                            <ul class="postedservices">
                              <% for(var i = 0; i < postedservices.length; i++) {%>
                              <li class="postedservices">
                                <span><%= postedservices[i].name %></span>:
                                <span><%= postedservices[i].category %></span>
                                <span><%= postedservices[i].description %></span>
                              </li>
                              <% } %>
                            </ul>
                          </section>
                    </tbody>
                </table>
            </form>
        </div>
    </main>
<!-- /Main Site -->


<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->